    },
    "/api/candidates/{candidate_id}/message": {
      "get": {
        "summary": "Get Candidate Message",
        "description": "Get outreach message for a candidate",
        "operationId": "get_candidate_message_api_candidates__candidate_id__message_get",
        "parameters": [
          {
            "name": "candidate_id",
            "in": "path",
            "required": true,
